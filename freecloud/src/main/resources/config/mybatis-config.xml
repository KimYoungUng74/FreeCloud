<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- [DB연결 사용법] 8. 만든 DTO와 Mapper정의하기 -->
	<typeAliases>
		<!-- <typeAlias type="com.spring.travel.dto.UserDTO" alias="UserDTO"></typeAlias> -->
		<typeAlias type="com.spring.freecloud.dto.UserDTO" alias="UserDTO"></typeAlias>
		<typeAlias type="com.spring.freecloud.dto.BoardDTO" alias="BoardDTO"></typeAlias>
		<typeAlias type="com.spring.freecloud.dto.ProjectDTO" alias="ProjectDTO"></typeAlias>
		<typeAlias type="com.spring.freecloud.dto.PortfolioDTO" alias="PortfolioDTO"></typeAlias>
			<typeAlias type="com.spring.freecloud.dto.MyfreelancerDTO" alias="MyfreelancerDTO"></typeAlias>
	</typeAliases>
	
	
	
	
	<mappers>
		<!-- <mapper resource="mappers/UserMapper.xml" /> -->
		<mapper resource="mappers/UserMapper.xml" />
		<mapper resource="mappers/BoardMapper.xml" />
	</mappers>
</configuration>
